           <Insets left="12.0" right="12.0" />
        </GridPane.margin>
     </Button>
     <Label fx:id="selectedSignFileLabel" text="No file selected">
        <GridPane.margin>
           <Insets left="148.0" />
        </GridPane.margin>
     </Label>
     <Button fx:id="verifyFileButton" layoutX="422.0" layoutY="13.0" minWidth="126.0" mnemonicParsing="false" onAction="#handleVerifyFileButtonAction" text="Verify signed file" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <GridPane.margin>
           <Insets left="12.0" right="12.0" />
        </GridPane.margin>
     </Button>
  <Button fx:id="selectVerifyFileButton" layoutX="22.0" layoutY="13.0" mnemonicParsing="false" onAction="#handleSelectVerifyFileButtonAction" prefWidth="126.0" text="Select a file..." GridPane.rowIndex="1">
     <GridPane.margin>
        <Insets left="12.0" />
     </GridPane.margin>
  </Button>
  <Label fx:id="selectedVerifyFileLabel" layoutX="158.0" layoutY="17.0" text="No file selected" GridPane.rowIndex="1">
     <GridPane.margin>
        <Insets left="148.0" />
     </GridPane.margin>
  </Label>
